<bindings>
  <remove-node path="/api/package[@name='com.useinsider.insider']/class[@name='BuildConfig']" />
  <add-node path="/api/package[@name='com.useinsider.insider']/class[@name='Insider']">
    <method name="Init" type="void" static="false">
      <parameter name="context" type="android.content.Context" />
      <parameter name="partnerName" type="java.lang.String" />
    </method>
