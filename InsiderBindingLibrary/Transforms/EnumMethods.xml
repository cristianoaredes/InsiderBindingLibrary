<enum-method-mappings>
  <!--
  This example changes the Java method:
    android.support.v4.app.Fragment.SavedState.writeToParcel (int flags)
  to be:
    android.support.v4.app.Fragment.SavedState.writeToParcel (Android.OS.ParcelableWriteFlags flags)
  when bound in C#.
  
  <mapping jni-class="android/support/v4/app/Fragment.SavedState">
    <method jni-name="writeToParcel" parameter="flags" clr-enum-type="Android.OS.ParcelableWriteFlags" />
  </mapping>
  -->
  <mapping jni-class="com/useinsider/insider/Insider">
    <method jni-name="getContentStringWithName" parameter="dataType" clr-enum-type="Com.Useinsider.Insider.ContentOptimizerDataType" />
    <method jni-name="getContentBoolWithName" parameter="dataType" clr-enum-type="Com.Useinsider.Insider.ContentOptimizerDataType" />
    <method jni-name="getContentIntWithName" parameter="dataType" clr-enum-type="Com.Useinsider.Insider.ContentOptimizerDataType" />
  </mapping>
</enum-method-mappings>
